@model  IEnumerable<ChurchFinanceSite.Models.Group>
@{
    ViewBag.Title = "Index";
}

<h2>Group</h2>
@Html.ActionLink("Create New Group", "Create")
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Group Name</th>
            <th>Date Created</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.GroupDateCreated</td>
            </tr>
        }
    </tbody>
</table>
